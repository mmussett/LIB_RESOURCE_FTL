<?xml version="1.0" encoding="UTF-8"?>
<project 
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" 
	xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

  <modelVersion>4.0.0</modelVersion>

  <groupId>com.royallondon.lib.resource</groupId>
  <artifactId>LIB_RESOURCE_FTL-implement.module</artifactId>
  <version>2.0.6.qualifier</version>
  
  <packaging>bwmodule</packaging>
  
  <build>
    <sourceDirectory>src</sourceDirectory>
    <outputDirectory>target/classes</outputDirectory>
    <plugins>
      <plugin>
        <groupId>com.tibco.plugins</groupId>
        <artifactId>bw6-maven-plugin</artifactId>
        <version>2.8.1</version>
        <extensions>true</extensions>
      </plugin>
    </plugins>
  </build>

  <distributionManagement>
    <repository>
      <id>${repoId}</id>
      <layout>${repoLayout}</layout>
      <url>${repoUrl}</url>
    </repository>
  </distributionManagement>
  
</project>
